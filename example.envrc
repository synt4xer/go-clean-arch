export APP_NAME="go-clean-arch"
export APP_VERSION="v0.0.1"
export APP_ENV="development"

export SERVER_PORT=5000
export SERVER_DEBUG=true
export SERVER_TIMEZONE="Asia/Jakarta"

export DB_HOST=127.0.0.1
export DB_PORT=3306
export DB_USERNAME="mysql"
export DB_PASSWORD="password"
export DB_NAME="clean_arch"

export GOOSE_DRIVER="mysql"
export GOOSE_DBSTRING="$DB_USERNAME:$DB_PASSWORD@tcp($DB_HOST:$DB_PORT)/$DB_NAME?parseTime=true"
export GOOSE_MIGRATION_DIR="database/migrations"